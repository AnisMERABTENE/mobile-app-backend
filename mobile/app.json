{
  "expo": {
    "name": "Mobile App",
    "slug": "mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#667eea",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Nouvelles demandes"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mobileapp.app",
      "scheme": "myapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#667eea"
      },
      "package": "com.mobileapp.app",
      "scheme": "myapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "myapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.VIBRATE"
      ],
      "notification": {
        "icon": "./assets/notification-icon.png",
        "color": "#667eea",
        "defaultChannel": "default"
      }
    },
    "scheme": "myapp",
    "extra": {
      "eas": {
        "projectId": "b458eb82-4050-449c-b1c0-a4c98242744f"
      },
      "googleOAuth": {
        "iosClientId": "1090312299997-hv7ic64it62ftednl1ft3sc39uugjr62.apps.googleusercontent.com",
        "androidClientId": "1090312299997-hv7ic64it62ftednl1ft3sc39uugjr62.apps.googleusercontent.com",
        "webClientId": "1090312299997-hv7ic64it62ftednl1ft3sc39uugjr62.apps.googleusercontent.com"
      }
    }
  }
}